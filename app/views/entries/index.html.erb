<h1>My Diary</h1>
<img src="https://avatars3.githubusercontent.com/u/15889670?v=3&s=400" />  

<div>
  <%= link_to("Create Post", new_entry_path) %>

    <% for entry in @entries %>
  <h2><%= link_to(entry.title, entry_path(entry)) %></h2>
    <p><%= entry.date %></p>
    <p><%= entry.content %></p>
<% end %>
  </div>
   <div>
  <h1>My favourite websites</h1>
    <ul>
<% for url, name in @websites %>
  <li><a href="<%= url %>"><%= name %></a></li>
<% end %>
    </ul>
  </div>    

 