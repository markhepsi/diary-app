<%= form_for(@entry) do |form| %>
  <p><%= form.label("title") %></p>
  <p><%= form.text_field("title") %></p>
  <p><%= form.label("content") %></p>
  <p><%= form.text_area("content") %></p>
  <p><%= form.submit %></p>
<% end %>
<p><%= link_to("Back", entry_path(@entry)) %></p>
<p><%= link_to("Home", entries_path) %></p>